{
  "name": "kontourcoin-dev",
  "version": "1.0.0",
  "description": "Development tools for Kontour Coin blockchain",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --mode production",
    "dev:web3": "cd ../web3 && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:web3\" \"npm run dev:backend\" \"npm run dev:java\"",
    "dev:backend": "cd ../backend && python backend.py",
    "dev:java": "cd ../java && ./gradlew bootRun",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "dashboard": "node dashboard.js",
    "update-deps": "node update-deps.js",
    "setup": "node index.js setup",
    "install-all": "npm install && cd ../web3 && npm install && cd ../backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "inquirer": "^8.2.4",
    "ora": "^5.4.1",
    "socket.io": "^4.5.2",
    "web3": "^1.8.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT"
